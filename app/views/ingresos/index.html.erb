<h1>Listing ingresos</h1>

<table>
  <tr>
    <th>User</th>
    <th>Cantidad</th>
    <th>Fecha</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ingresos.each do |ingreso| %>
  <tr>
    <td><%= ingreso.user_id %></td>
    <td><%= ingreso.cantidad %></td>
    <td><%= ingreso.fecha %></td>
    <td><%= link_to 'Show', ingreso %></td>
    <td><%= link_to 'Edit', edit_ingreso_path(ingreso) %></td>
    <td><%= link_to 'Destroy', ingreso, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Ingreso', new_ingreso_path %>
